<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ResultInfoActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="130dp"
        android:layout_marginLeft="20dp" />

    <TextView
        android:id="@+id/raspunsuriCorecte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Raspunsuri corecte."
        android:layout_marginTop="170dp"
        android:layout_marginLeft="20dp"
        android:textColor="@color/green"/>

    <TextView
        android:id="@+id/raspunsuriGresite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Raspunsuri gresite."
        android:layout_marginTop="210dp"
        android:layout_marginLeft="20dp"
        android:textColor="@color/red"/>

    <Button
        android:id="@+id/buttonExamineaza"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="250dp"
        android:text="Examineaza raspunsuri gresite" />
</RelativeLayout>